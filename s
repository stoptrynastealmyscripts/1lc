function getClosestPlayerToCursor()local a;local b=circle.Radius;for c,d in pairs(game.Players:GetPlayers())do if d~=game.Players.LocalPlayer and d.Character and d.Character:FindFirstChild("Humanoid")and d.Character.Humanoid.Health~=0 and d.Character:FindFirstChild("LowerTorso")then local e=CC:WorldToViewportPoint(d.Character.PrimaryPart.Position)local f=(Vector2.new(e.X,e.Y)-Vector2.new(LocalMouse.X,LocalMouse.Y)).magnitude;if f<b then a=d;b=f end end end;return a end;if getgenv().PartMode then game:GetService"RunService".Stepped:connect(function()if Locking and Plr.Character and Plr.Character:FindFirstChild("LowerTorso")then Tracer.CFrame=CFrame.new(Plr.Character.LowerTorso.Position+Plr.Character.LowerTorso.Velocity*Prediction)else Tracer.CFrame=CFrame.new(0,9999,0)end end)end;local g=getrawmetatable(game)local h=g.__namecall;setreadonly(g,false)g.__namecall=newcclosure(function(...)local i={...}if Locking and getnamecallmethod()=="FireServer"and i[2]=="UpdateMousePos"then i[3]=Plr.Character[getgenv().Partz].Position+Plr.Character[getgenv().Partz].Velocity*Prediction;return h(unpack(i))end;return h(...)end)while wait()do if getgenv().AutoPrediction==true then local j=game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()local k=string.split(j,'(')local l=tonumber(k[1])if l<130 then getgenv().Prediction=0.151 elseif l<125 then getgenv().Prediction=0.149 elseif l<110 then getgenv().Prediction=0.140 elseif l<105 then getgenv().Prediction=0.133 elseif l<90 then getgenv().Prediction=0.130 elseif l<80 then getgenv().Prediction=0.128 elseif l<70 then getgenv().Prediction=0.1230 elseif l<60 then getgenv().Prediction=0.1229 elseif l<50 then getgenv().Prediction=0.1225 elseif l<40 then getgenv().Prediction=0.1256 end end end
